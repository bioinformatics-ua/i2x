<h4 class="subheader">Events</h4>
<ul class="no-bullet">
	<% @events.each do |event| %>
	<li><%= link_to "#{event.agent.title}".truncate(10, :omission => "... ").html_safe, event, :class => 'icon-right' %> <span class="tiny label secondary radius"><%= time_ago_in_words(event.created_at) %> ago </span></li>
	<% end %>
</ul>
